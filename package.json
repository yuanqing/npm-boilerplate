{
  "name": "npm-boilerplate",
  "version": "0.0.0",
  "description": "Barebones boilerplate with `npm scripts`.",
  "author": "Lim Yuan Qing",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git://github.com/yuanqing/npm-boilerplate.git"
  },
  "devDependencies": {
    "istanbul": "^0.3.2",
    "jshint": "^2.5.10",
    "jshint-stylish": "^1.0.0",
    "matcha": "^0.6.0",
    "minijasminenode2": "^1.0.0"
  },
  "scripts": {
    "build": "npm run lint && npm test && npm run cover && npm run bench",
    "bench": "node_modules/.bin/matcha benchmark.js",
    "lint": "node_modules/.bin/jshint --verbose --reporter=node_modules/jshint-stylish/stylish.js benchmark.js index.js test.js",
    "test": "node_modules/.bin/minijasminenode2 --verbose test.js",
    "cover": "node_modules/.bin/istanbul cover node_modules/.bin/minijasminenode2 test.js"
  },
  "keywords": [
    "boilerplate"
  ]
}
